# Firebase Functions Environment Variables
# Copy this file to .env and fill in your values (for local testing)
# For production, set these via: firebase functions:config:set key=value

# Twilio Credentials (get from https://console.twilio.com)
TWILIO_ACCOUNT_SID=your_twilio_account_sid
TWILIO_AUTH_TOKEN=your_twilio_auth_token
TWILIO_PHONE_NUMBER=+1234567890

# Admin phone numbers to notify (comma-separated, include country code)
ADMIN_PHONE_NUMBERS=+94771234567,+94779876543
